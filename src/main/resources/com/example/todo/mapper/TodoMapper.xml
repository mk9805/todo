<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.todo.mapper.TodoMapper">
<select id="selectAll" resultType="com.example.todo.domain.Todo">
SELECT * FROM todo
</select>
<insert id="insertTodo" parameterType="com.example.todo.domain.Todo">
INSERT INTO todo (id,title,deadline)
VALUES(#{id},#{title},#{deadLine})
</insert>
</mapper>